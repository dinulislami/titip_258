<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".register">

    <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            tools:layout_editor_absoluteX="189dp">

        <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:id="@+id/textView"
                android:background="#00BCD4"
                android:text="Form Biodata"
                android:textAlignment="center"
                android:textColor="#000000"/>
        <EditText
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@id/textView"
                android:id="@+id/txt_nama"
                android:ems="10"
                android:inputType="textPersonName"
                android:text="Nama"/>
        <EditText
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@id/txt_nama"
                android:id="@+id/txt_ttl"
                android:ems="10"
                android:inputType="textPersonName"
                android:text="Tempat dan tanggal Lahir"/>
        <EditText
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@id/txt_ttl"
                android:id="@+id/txt_alamat"
                android:ems="10"
                android:inputType="textPersonName"
                android:text="Alamat"/>
        <EditText
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@id/txt_alamat"
                android:id="@+id/txt_email"
                android:ems="10"
                android:inputType="textPersonName"
                android:text="Email"/>
        <EditText
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@id/txt_email"
                android:id="@+id/txt_password"
                android:ems="10"
                android:inputType="textPassword"
                android:text="Password"/>
        <Button
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:id="@+id/btn_proccess"
                android:layout_below="@id/txt_password"
                android:layout_marginTop="9dp"
                android:text="PROCESS" />

    </RelativeLayout>


</LinearLayout>